{
  "name": "cds-nats",
  "version": "2.9.1",
  "main": "lib/index.js",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "test": "jest --passWithNoTests --runInBand",
    "install:dev": "bash install_no_trace_deps.sh",
    "pretest": "npm run install:dev",
    "prelint": "npm run install:dev",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "16.2.4",
    "@types/jest": "27.5.2",
    "@types/node": "16.11.41",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "commitlint": "16.3.0",
    "eslint": "8.18.0",
    "eslint-plugin-jest": "26.5.3",
    "husky": "4.3.8",
    "jest": "27.5.1",
    "jest-junit": "13.2.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.5",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-nats"
  },
  "dependencies": {
    "@newdash/newdash": "^5.21.2",
    "cds-internal-tool": "^1.1.2",
    "nats": "^2.7.1"
  },
  "overrides": {
    "@sap/xsenv": "3.3.2"
  },
  "license": "Apache-2.0",
  "keywords": [
    "cap",
    "cds",
    "nats",
    "mq",
    "messaging"
  ]
}
