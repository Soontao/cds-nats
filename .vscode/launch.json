{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "vscode-jest-tests",
      "request": "launch",
      "args": [
        "--runInBand"
      ],
      "outputCapture": "std",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest"
    }
  ]
}
