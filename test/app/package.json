{
  "cds": {
    "requires": {
      "messaging": {
        "kind": "nats"
      },
      "kv": {
        "kind": "nats-kv",
        "ttl": 100
      },
      "kv5000": {
        "kind": "nats-kv",
        "ttl": 5000
      },
      "lock": {
        "kind": "nats-lock",
        "check": {
          "interval": 10
        },
        "lock": {
          "acquire": 10000
        }
      },
      "lock1000": {
        "kind": "nats-lock",
        "check": {
          "interval": 10
        },
        "lock": {
          "timeout": 1000,
          "acquire": 10000
        }
      },
      "rfc": {
        "kind": "nats-rfc",
        "app": {
          "name": "demo-app-micro-service"
        },
        "invoke": {
          "timeout": 180000
        }
      },
      "nats": {
        "impl": "../../src"
      },
      "nats-kv": {
        "impl": "../../src/NatsKVService"
      },
      "nats-lock": {
        "impl": "../../src/NatsLockService"
      },
      "nats-rfc": {
        "impl": "../../src/NatsRFCService"
      }
    }
  }
}
